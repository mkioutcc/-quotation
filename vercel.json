{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/login.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/login.html",
      "status": 200
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/login.html"
    }
  ]
}
